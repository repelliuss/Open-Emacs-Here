Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\*\shell\Open with Emacs]
@="Open &with &Emacs"
"Icon"="$"

[HKEY_CLASSES_ROOT\*\shell\Open with Emacs\command]
@="\"$\" \"%1%\" '%1' '%2'"

[HKEY_CLASSES_ROOT\Directory\Background\shell\Open Emacs Here]
@="Open &Emacs &Here"
"Icon"="$"

[HKEY_CLASSES_ROOT\Directory\Background\shell\Open Emacs Here\command]
@="\"$\" '%V'"
